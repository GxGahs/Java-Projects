public class Generator 
{
    private int seed;
    
    public Generator(double givenSeed)
    {
        if (Double.toString(givenSeed).length() > 32)
            givenSeed = Double.parseDouble(Double.toString(givenSeed).substring(0,32));
        else
        {
            while (Double.toString(givenSeed).length() > 32)
            {
                givenSeed *= Math.pow(87,);
            }
        }
        
        
    }
    
}
