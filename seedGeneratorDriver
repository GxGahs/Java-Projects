public class Driver
{

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) 
    {
        String seed = "";
        for (int i = 0; i < 32; i++)
        {
            seed += Integer.toString((int)(Math.random()*10));
        }
        Generator newSeed = new Generator(seed);
        
        newSeed.toEight(newSeed.toSixteen());
        
        
        
        
        
    }
}
