class Move
{
    BufferedReader br = null;
        FileReader fr = null;
    private String name;
    private int power,ID;
    
    public Move ()
    {
        
    }
    
    public void setMove(int num, String input)

    {
        try {
			fr = new FileReader(input);
			br = new BufferedReader(fr);
		} catch (FileNotFoundException e) { System.out.println("FileNotFound");}

        String mAS; //moveAsString
        try {
            for (int i = 0; i < 7 + num; i++)
                br.readLine();
            mAS = br.readLine();
            if (mAS == null) {
                ID = -1;
                power = 0;
            }
            else {
            ID = Integer.parseInt(mAS.substring(0,mAS.indexOf(' ')));
            power = Integer.parseInt(mAS.substring(mAS.indexOf(' ')+1));
            System.out.println(ID);
            System.out.println(power);
            }
            
            } catch (IOException e) {}
            finally {
            try {
            if (br != null)
				br.close();

			if (fr != null)
				fr.close();
            } catch (IOException e) {}
        }
        
    }
    public void getMove()
    {
    	System.out.println("(" + ID + ", " + power + ")");
    }
}
